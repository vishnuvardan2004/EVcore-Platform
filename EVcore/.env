# EVcore Frontend Development Environment Variables

# Backend API Configuration (Development)
VITE_API_URL=http://localhost:3001
VITE_WS_URL=ws://localhost:3001
VITE_UPLOAD_URL=http://localhost:3001/upload

# Production URLs (commented out for development)
# VITE_API_URL=https://evcore-backend.vercel.app
# VITE_WS_URL=wss://evcore-backend.vercel.app
# VITE_UPLOAD_URL=https://evcore-backend.vercel.app/upload

# Feature Flags
VITE_ENABLE_REALTIME=true
VITE_ENABLE_UPLOAD=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true

# Environment
MODE=development

# Build Configuration
VITE_BUILD_DATE=2025-08-30
VITE_VERSION=1.0.0

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security
VITE_ENABLE_STRICT_CSP=true
VITE_ENABLE_SOURCE_MAPS=false

# Analytics & Monitoring
VITE_ENABLE_ERROR_REPORTING=true
